{
  "name": "kapina-frontend",
  "version": "1.0.0",
  "description": "Frontend project for https://radiorevolt.no",
  "homepage": "https://radiorevolt.no",
  "keywords": [
    "radiorevolt",
    "radio",
    "revolt",
    "kapina",
    "studentmediene",
    "samfundet",
    "trondheim"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Studentmediene/revolt-frontend"
  },
  "engines": {
    "node": ">=8.1.0"
  },
  "author": "Vemund Santi <vemund@santi.no>",
  "contributors": [
    "Boye Nyg√•rd",
    "Edvin Johnsrud",
    "Petter Dybdal",
    "Thorben Dahl"
  ],
  "scripts": {
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean && npm run test",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "precommit": "lint-staged && npm run test",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/",
    "lint:css": "stylelint ./src/**/*css",
    "test": "jest src/"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings 0 --ignore-pattern !.eslintrc.js"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/internals/testing/jestsetup.js"
    ],
    "automock": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "[/\\\\]build[/\\\\].+\\.(js|jsx)$",
      "<rootDir>/internals/",
      "<rootDir>/server/"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/internals/testing/transforms/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/internals/testing/transforms/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "connected-react-router": "^6.5.2",
    "express": "^4.16.3",
    "history": "^4.9.0",
    "immutable": "^3.8.2",
    "invariant": "^2.2.4",
    "ip": "^1.1.5",
    "lodash": "^4.17.15",
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "raven-js": "^3.24.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.6.0",
    "react-lazyload": "^2.6.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.4.8",
    "redux": "^4.0.4",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.0.5",
    "reselect": "^3.0.1",
    "sanitize.css": "^5.0.0",
    "soundmanager2": "^2.97.20170602",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "cheerio": "0.20.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.0",
    "moment-locales-webpack-plugin": "^1.0.5",
    "ngrok": "^3.1.1",
    "node-sass": "^4.9.4",
    "object-assign": "^4.1.1",
    "offline-plugin": "^5.0.5",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "promise": "^8.0.1",
    "react-test-renderer": "^16.3.1",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.0",
    "whatwg-fetch": "^2.0.4"
  }
}
